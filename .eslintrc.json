{
   "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
     "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType":"module"
     },
     "overrides": [
        {
        "files": [
            "**/*.test.js",
            "**/*.test.jsx"
        ],
        "env": {
            "jest": true
        }
        }
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "react/prop-types": "off",
        "react/display-name": "off",
        "no-unused-vars": "warn",
        "no-debugger": "warn"
    }
}